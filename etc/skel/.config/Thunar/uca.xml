<actions>
<action>
	<icon>xterm-color</icon>
	<name>Open Terminal Here</name>
	<unique-id>1448574952005744-3</unique-id>
	<command>exo-open.exe --launch TerminalEmulator --working-directory %f</command>
	<description>Xfce Terminal emulator</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>xterm-color</icon>
	<name>Open Command Prompt Here</name>
	<unique-id>1442281116744808-4</unique-id>
	<command>/bin/exo-start cmd /K cd &quot;$(cygpath -w %f)&quot;</command>
	<description>Microsoft exo-start cmd.exe</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>wine</icon>
	<name>Open with Microsoft Windows</name>
	<unique-id>1442606331661533-1</unique-id>
	<command>/bin/exo-start %F</command>
	<description>Use Microsoft Windows filetype association</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
